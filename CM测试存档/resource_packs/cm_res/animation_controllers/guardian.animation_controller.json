{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.cm.guardian.move.jump" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"animations" : [
						"swim",
						"walk",
						"look_at_target",
						{
							"jump" : "query.vertical_speed > 0.04 || query.vertical_speed < -0.04 && query.is_in_water == 0"
						}
					]
				}
			}
		}
		
	}
}
